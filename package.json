{
  "name": "my-vue-app",
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "build:server": "vite build --ssr src/main-server.tsx --outDir dist/server",
    "generate": "rimraf ./dist && vite build --outDir dist/static && npm run build:server && node prerender",
    "serve": "vite preview",
    "deploy": "node deploy"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "rc-scrollbars": "^1.1.3",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "gh-pages": "^3.2.0",
    "node-scp": "^0.0.15",
    "ora": "^5.4.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.2",
    "vite": "^2.3.0"
  }
}
